.textarea
{
	margin:0 auto;
	background:#fff;
	margin-top:44px;
}
.text
{
	width:100%;
	padding:15px 2.5% 15px 2.5%;
	border-bottom:1px solid #dfdfdf;
}
.title1
{
	font-size:16px;
	color: #000;
	line-height:16px;
}
.title2
{
	font-size:12px;
	margin-top:12px;
	color: #000;
	line-height:12px;
}
.title2 span{
	float:right;
	color:#8b8b8b;
	font-size:12px;
}
.title3
{
	font-size:12px;
	margin-top:11px;
	color:#8b8b8b;
	line-height:12px;
}
.content
{
	padding-top:20px;
	padding-bottom:15px;
	background:#fff;
	border-bottom:10px solid #e8e8e8;
}
.content-inner
{
	margin:0 auto;
	width:95%;
}
.content-inner img
{
	width:17%;
	float:left;
}
.doctor_info
{
	width:83%;
	float:left;
	padding-left:10px;
	
}
.clear
{
	clear:both;
}
.xingming
{
	font-size:16px;
	color: #000;
	margin-bottom:15px;
	line-height:16px;
}

.doctor_info .orange
{
	font-size:12px;
	color:#db8f18;
	margin-left:8%;
}
.doctor_info .red
{
	font-size:12px;
	color:#ff000;
	float:right;
}
.keshi
{
	font-size:12px;
	color:#8b8b8b;
	line-height:12px;
}
.yishi
{
	font-size:12px;
	color:#8b8b8b;
	line-height:12px;
	margin-left:7%;
}
.address{
	margin-top:20px;
	color:#8b8b8b;
	font-size:12px;
	line-height:24px;
	margin-top:6px;
}
.area{
	font-size:12px;
	line-height:12px;
	color:#000;
	margin-top:1px;
}
.address span{
	font-size:14px;
	color:#000;
}
.grjl{
	font-size:12px;
	color:#8b8b8b;
	line-height:24px;
}
.grjl span{
	font-size:14px;
	color:#000;
}
.img-hb
{
	padding-top:10px;
	padding-bottom:10px;
	background:#e8e8e8;
}
.img-hb img{
	width:190px;
	position:relative;
	top:1.5px;
}
.service
{
	background:#fff;
	padding-top:12px;
	padding-bottom:12px;
	padding-left:2.5%;
	padding-right:2.5%;
	line-height:15px;
}
.service .sev{
	height:15px;
	line-height:15px;
}
.service .serv .sd
{
	font-size:15px;
	float:left;
	color: #000;
}
.service .know
{
	float:right;
	position:relative;
	top:2px;
}
.service .know span:hover{
	color: #000;
}
.know img{
	width: 13px;
	margin-right:-3px;
}
.know span{
	position:relative;
	font-size:13px;
	top:-2px;
}
.service-other
{
	background:#fff;
	padding-top:16px;
	padding-bottom:16px;
	padding-left:2.5%;
	line-height:14px;
	position: relative;
	border-bottom:1px solid #dfdfdf;
}
.service-other .red-line{
	width: 4px;
	height: 14px;
	background: red;
	position: absolute;
	left: 0;
}
.service-other p 
{
	font-size:14px;
	color:#000;
}
.jieshao
{
	background:#e8e8e8;
	padding-top:10px;
	padding-bottom:10px;
}
.wenzi
{
	font-size:14px;
	width:95%;
	margin:0 auto;
	text-indent: 2em;
	min-height:280px;
}
.btn
{
	padding-top:10px;
	padding-bottom:10px;
	width:100%;
	background: #fff;
	margin-top:22px;
	/*box-shadow:  0 -2px 4px #e1e1e1;*/
}
.btn1
{
	width: 95%;
	margin: auto;
}
.canyu
{
	font-size: 16px;
	color:#fff;
	background:#30c6df;
	padding:8px 8px;
	border-radius:5px;
	float:left;
	cursor: pointer;
}
.canyu:hover{
	position: relative;
	left: 1px;
	top: 1px;
}
.btn a:hover{
	color: #fff;
}
.detail-list{
	padding-bottom: 52px;
}
.detail-list img{
	width: 100%;
}
.buy
{
	font-size: 13px;
	color:#fff;
	background:#30c6df;
	padding:6px 6px;
	border-radius:5px;
	cursor: pointer;
	text-align:center;
}
.buy:hover{
	position: relative;
	left: 1px;
	top: 1px;
}
.arrow-right{
	width: 44px;
    height: 44px;
    position: absolute;
    right: 0;
    top: 30px;
    padding-left: 22px;
    cursor: pointer;
}
.arrow-right img{
	width:22px;
}
.otherdoc{
	width:100%;
	height:48px;
	padding:0 2.5% 0 2.5%;
	
}
.rec{
	width:4px;
	height:14px;
	background:#ff000;
}
.content2
{
	padding-top:20px;
	padding-bottom:15px;
	background:#fff;
}