.detail-head .p1{
	line-height: 40px;
	margin-top: 10px;
	color: #000;
	font-size: 17px;
}
.detail-head .p2{
	color: #333;
}
.detail-head .p2 span{
	color: #ff0000;
	line-height: 30px;
	font-size: 16px;
	font-weight: 700;
}
.nav{
	width: 100%;
	height: 10px;
	background: #aaaaaa;
	margin:20px auto;
}
.retitle{
	width: 100%;
	border: 1px solid #E43D37;
	border-radius: 5px;
	margin-bottom: 10px;
}
.retitle a{
	width:33.33333%;
	float: left;
	text-align: center;
	display: block;
	color: #777;
	font-size: 16px;
	line-height: 30px;
}
.retitle .active{
	background: #E43D37;
	color: #fff;
}
.retitle .a1{
	border-right: 1px solid #E43D37;
}
.assign{
	padding:10px 0 10px 0;
	border-bottom:1px solid #bbb;
	width: 100%;
	position: relative;
}
.aleft{
	float: left;
	width: 17%;
}
.aleft img{
	width: 100%;
	border-radius: 10px;
}
.aleft-view{
	float: right;
	width: 83%;
	padding-left: 10px;
}
.aleft-view .p1{
	color: #333;
	line-height: 30px;
	font-size:16px;
}
.aleft-view .p2{
	margin-top: 6px;
}
.right-staff{
	position: absolute;
	right: 0;
	top:0;
	width: 6em;
	text-align: right;
	font-size: 15px;
	color: #666;
}
.right-staff img{
	width: 2em;
	height: 2em;
	position: relative;
	top:12px;
	border-radius: 20px;
}
