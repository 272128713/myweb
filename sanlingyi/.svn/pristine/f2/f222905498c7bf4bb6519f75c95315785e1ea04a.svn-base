
/**
 *用户看自己的记录
 */

.mui-table-view-cell>a:not(.mui-btn){
	white-space: normal;
}


.ra{
	border-radius:10px;
}

.wen{
	float:left;
	width:1em;
	margin-top:0.2em;

}
.ti{
	margin-left: 0.2em;
	font-size: 14px;
	float:left;
	width:85%;
	color:#000;
}
.tifoot{
	margin-top:0.5em;
	color:#666;
	font-size:12px;
}
.tifoot span{
	font-size:12px;
	min-width: 2em;
}


/**
 *选项卡 
 */
.mui-segmented-control{
	border: 1px solid #30C6DF;
	background:#fff;
	border-radius:2em;
}
.mui-segmented-control .mui-control-item.mui-active{
	background-color: #30c6df;
	border-radius:1em 0 0 1em;
}
.mui-segmented-control .mui-control-item.mui-active1{
	background-color: #30c6df;
	border-radius:0 1em 1em 0;
	color: #fff;
}
.mui-segmented-control .mui-control-item{
	border-color: #30c6df;
}
.mui-segmented-control .mui-control-item{
	color:#666666;
}
.cc{
	width:40%;height:10px;margin: 0em auto 0.8em auto;
	position:relative;
	z-index:1;
}
.hdline{
	width:100%;height: 1px;
	position:relative;
	top:1.5em;
	z-index:0;
}