	<script>
		apiready = function() {
			api.closeFrame({name:api.pageParam.name});
			$header = $api.dom('.header');
			$api.fixStatusBar($api.dom('.header'));
			$header_h = $api.offset($header).h;
			gid = api.pageParam.gid;
			price = api.pageParam.price;
			api.openFrame({
				name : 'forderok',
				url : 'frame_order_ok.html',
				rect : {
					x : 0,
					y : $header_h,
					w : 'auto',
					h : 'auto'
				},
				pageParam:{
					gid:gid,
					price:price,
				}
			});
		}
	</script>
