.order .icon2{
	width:15px;
	height:15px;
	background-image: url(../images/order.png);
	background-size: 15px 15px;
	background-position: center;
	margin-top:0px;
	display:block;
	position:relative;
}
.sort_show{
	width:100%;
	margin:0 auto;
	padding-left:2.5%;
	padding-right:2.5%;
	/*border-bottom:5px solid #ededed;*/
}
.sort_show .sort_title,.sort_show .sort_bao{
	width:100%;
}
.sort_show .sort_title{
	height:28px;
	padding-top:6.5px;
	padding-bottom:6.5px;
	position:relative;
}
.sort_title .sort_icon{
	width:25px;
	height:15px;
	background-image: url(../images/car.png);
	background-repeat: no-repeat;
	background-size: 25px 15px;
	background-position: center;
	position:relative;
	display:inline-block;
}
.sort_title .sort_icon2{
	width:15px;
	height:15px;
	background-image: url(../images/home.png);
	background-repeat: no-repeat;
	background-size: 15px 15px;
	background-position: center;
	top:-2px;
	position:relative;
	display:inline-block;
}
.sort_title .name{
	color:#dd403b;
	font-size: 13px;
	position:relative;
	top:-3px;
}

.sort_title .arrowright{
	width:28px;
	height:28px;
	background-image: url(../images/next.png);
	background-repeat: no-repeat;
	background-size: 7px 13px;
	background-position:center ;
	top:0;
	right:0;
	position:absolute;
	display:inline-block;
}
.sort_show .sort_bao{
	border-top:1px solid #a0a0a0;
}
.sort_bao .sLeft{
	width:36%;
	float:left;
}
.sort_bao .sRight{
	width:64%;
	float:left;
	border-left:1px solid #a0a0a0;
}
.sLeft img{
	width:100%;
	position:relative;
}
.sRight .top,.sRight .bottom{
	width:100%;
	padding-top:5px;
}
.sRight .top{
	border-bottom:1px solid #a0a0a0;
}
.sRight .top .left{
	width:40%;
	float:left;
}
.sRight .top .left img{
	width:81%;
	position:relative;
}
.sRight .top .right{
	width:60%;
	float:left;
}
.sRight .bottom .left{
	width:40%;
	float:left;
}
.sRight .bottom .left img{
	width:81%;
	position:relative;
}
.sRight .bottom .right{
	width:60%;
	float:left;
}
.sRight p{
	font-size:12px;
}
.sRight .right .bname{
	color:#1e1e1e;
}
.sRight .right .intro{
	color:#6a6969;
	display:-webkit-box;
	-webkit-box-orient:vertical;
	-webkit-line-clamp: 2;
	overflow:hidden;
}
.sLeft  .heading{
	font-size:12px;
	
	color:#1e1e1e;
	display:block;
}
.border{
	width:100%;
	height:5px;
	background:#ededed;
}
.guess{
	width:100%;
	padding-bottom:20px;
}
.guess .p1{
	color:#1e1e1e;
	font-size:13px;
	text-align:center;
	margin-top:10px;
	margin-bottom:10px;
}
.guess .relation{
	width:100%;
}
.relation .product{
	width:38%;
	float:left;
	margin-left:8%;
	border:1px solid #a6a6a6;
}
.product2{
	margin-left:8%;
}
.product img{
	width:100%;
}
.mingc{
	font-size:12px;
	color:#1e1e1e;
	text-align:center;
}
.jies{
	font-size:12px;
	color:#6a6969;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	padding-left:5px;
}
.mui-slider-indicator{
	bottom:0px;
}
.mui-slider-indicator .mui-indicator{
	background:#c8c8c8;
}
.mui-slider-indicator .mui-active.mui-indicator {
    background: #dd403b;
    box-shadow:0 0 1px 1px rgba(221,64,59,.7);
}
