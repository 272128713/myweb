/*
头条列表css
 * 
 * */
html,
body {
	background-color: #ffffff;
	font-family: "微软雅黑";
}
header ul li{
	list-style: none;
	float: left;
}
a{
	color: #5f5e5e;
}
a:hover{
	color: #5f5e5e;	
}
a:visited{
	color: #5f5e5e;	
}
.mui-bar~.mui-content .mui-fullscreen {
	top: 44px;
	height: auto;
}
.clear{
	clear: both;
}
/*
 * head
 */
header{
	height: 84px;
	line-height: 84px;
}

header img{
	width: 40%;
	max-width: 200px;
	margin-left:0.5rem;
	vertical-align: middle;
	margin-top:-0.6rem;
}
.ul-head{
	line-height: 84px;
	height: 84px;
	margin: 0;
	padding: 0;
	font-size:15px;

}
.li1{
	margin-right: 0.5rem;
}
.hui{
	color: #cccccc;
}
.ul-head .mui-icon{
	height: 84px;
	line-height: 84px;
	padding-right: 0.5rem;
	padding-left: 0.5rem;
	color: #5f5e5e;
}
.active{
	color:#000
}

/*
 * content
 */

.content{
	width: 93%;
	margin:0 auto;
	
}
.banner{
	background: #008e46;
	padding-bottom: 1.2rem;
}
.p1{
	color:#fff;
	font-size: 1.1rem;
	line-height: 2rem;
	padding-top: 0.7rem;
}
.p2{
	color:#fff;
	padding-bottom: 0.7rem;	
}
.banner-img img{
	width: 90%;
}
.banner-tc img{
	height: 6rem;
	padding-right: 1rem;
}
.content ul li {
	list-style-image: url(../img/liimg1.png)
}
.con-con{
	margin-top: 1rem;
	padding-top:0.2rem;
}
.con-con img{
	width:15%;
	max-width: 47px;
}
.con-p{
	width: 85%;
	padding-top:0.8rem;
	padding-left: 0.5rem;
}
.p11{
	font-size: 1rem;
	color:#000;
}
.hr{
	width: 100%;
	background: #CCCCCC;
	height: 1px;
	clear: both;
	margin-bottom: 1.2rem;
	margin-top: 1rem;
}
.footer{
	width: 93%;
	margin:0 auto;
	text-align: center;
}
.footer p{
	font-size: 12px;
}
/*
 * hd
 */
.green{
	color: #008E46;	
}
.hrg{
	width: 100%;
	background: #008E46;
	height: 1px;
	clear: both;
	margin-bottom: 1.2rem;
	margin-top: 1rem;	
}
.hdp1{
	background:#008E46 ;
	color:#fff;
	font-size: 1.2rem;
	line-height: 1.8rem;
	padding-top:0.5rem;
	padding-bottom:0.5rem;
}
.hdp2{
	margin-top:1.2rem;
	color: #008E46;
}
.indent{
	text-indent: 2rem;
}
