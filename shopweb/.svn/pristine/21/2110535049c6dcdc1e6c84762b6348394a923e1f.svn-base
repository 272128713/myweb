.right img{
	height: 22px;
	top:4px;
	position:relative;
	cursor: pointer;
}
.userinfo{
	width:100%;
	background:url(../images/beijing.png);
	backrtound-repeat:no-repeat;
	padding-bottom:35px; 
}
 .userinfo .contain{
	width:95%;
	margin:auto;
	margin-top:16px;
}
.userinfo .contain .userphoto{
	width:20%;
	border:1px solid #fff;
	border-radius:50%;
	float:left;
}
.userinfo .contain .moreinfo{
	width:80%;
	float:left;
	padding-left:3%;
}
.moreinfo p{
	color:#fff;
}
.moreinfo p span{
	color:#fff;
}
.moreinfo .nam{
	font-size:15px;
}
.moreinfo .icon{
	width:12px;
	margin-left:2%;
	margin-right:2%;
	top:1px;
	position:relative;
}
.moreinfo .sex{
	margin-right:2%;
	font-size:12px;
}
.moreinfo .year{
	font-size:12px;
}
.moreinfo .address{
	font-size:12px;
	line-height:18px;
	margin-top:6px;
}
.moreinfo .tel{
	font-size:12px;
	line-height:12px;
	margin-top:9px;
}
.moreinfo .tel img{
	position:relative;
	top:5px;
}
.moreinfo .doct{
	color:#fff;
	font-size:13px;
	margin-top:12px;
}
.moreinfo .doct img{
	height:13px;
	position:relative;
	top:2px;
}
