
.head{
	width:100%;
	height:44px;
	line-height:44px;
	position:fixed;
	top:0;
	left:0;
	background:#fff;
	z-index:100;
	text-align:left;
	padding:0px 2.5% 0px 2.5%;
}
.head a{
	width:44px;
	height:44px;
}
.head .title{
	color:#01a789;
	font-size:16px;
}
.mui-slider{
	
}
.pro_profile{
	width:100%;
	background:#fff;
	padding:15px 2.5% 10px 2.5%;
}
.pro_profile .title{
	font-size:15px;
	margin-bottom:15px;
	color:#000;
	line-height:15px;
}
.pro_profile p{
	
	line-height:12px;
	font-size:12px;
}
.oriprice{
	color:#ff0000;
	font-size:20px;
	line-height:20px;
	position:relative;
	top:1px;
	left:-2px;
}
.moborder{
	color:#fff;
	font-size:12px;
	background:#01a789;
	border-radius: 3px;
	margin-left:2%;
	padding: 0;
}
.proshow{
	width:95%;
	margin:auto;
	text-align:center;
	padding:15px 2% 0 2%;
	border-top:1px solid #dfdfdf;
	display:none;
}
.proshow .prod{
	width:33.33333333%;
	float:left;
	padding-left:1.5%;
	padding-right:1.5%;
}
.proshow img{
	width:100%;
}
.prod p {
    text-align: left;
    line-height: 13px;
    font-size: 13px;
    color: #000;
    margin-top: 2px;
}
.proname{
	overflow:hidden;
	white-space:nowrap;
	width:100%;
	text-overflow:ellipsis;
}
.prod .pri{
	color:#ff0000;
	margin-top:4px;
}
.proshow .taocan{
	width:97.5%;
	margin:auto;
	text-align:left;
	position:relative;
	margin-top:10px;
}
.taocan{
	border-top:1px solid #dfdfdf;
	padding-top: 18px;
	position:relative;
}
.taocan div{
	width:100%;
	height:33px;
}
.taocan .p2{
	position:relative;
	font-size:12px;
	line-height:12px;
	color: #666;
}
.p2 b{
	color:red;
	font-weight:400;
	font-size:20px;
	line-height:20px;
}
.p2 span{
	padding: 4px 4px 4px 4px;
	border-radius: 5px;
	color:#fff;
	background:#999;
	margin-left:3%;
}
.p2 u{
	color: red;
}
.taocan .p3{
	text-align:left;
	color:#666;
	font-size:12px;
	line-height:12px;
	position:relative;
}
.taocan .buybtn{
	text-align:center;
	width:30%;
	font-size:15px;
	line-height:30px;
	background:#01a789;
	color:#fff;
	position:absolute;
	right:0;
	top:9px;
	border-radius: 3px;
}
.intro-title{
	margin-top:26px;
	height:40px;
	line-height:40px;
	background:#e7e7e7;
}
.rect{
	width:5px;
	height:20px;
	background:#01a789;
	display:inline-block;
	margin-right:5px;
	position:relative;
	top:4px;
}
.intro-title .title{
	font-size:16px;
	
}
.proinfo{
	width:100%;
	margin:auto;
	padding:15px 2.5% 50px 2.5%;
}
.proinfo img{
	width:100%;
	margin-top:10px;
}
.bottomsec{
	width:100%;
	position:fixed;
	bottom:-1px;
}
.bottomsec .content{
	width:100%;
	margin:auto;
	height:40px;
}
.content a{
	display:block;
	line-height:40px;
	width:33.333333%;
	float:left;
	text-align:center;
	font-size:14px;
	color:#000;
	cursor: pointer;
}
.content .a1{
	border-right:1px solid #e7e7e7;
	border-top:1px solid #e7e7e7;
}
.content img{
	width:18px;
	position:relative;
	top:3px;
}
.content .active{
	background:#01a789;
	color:#fff;
}
.hid-pl{
	width: 100%;
	padding: 9px 0 9px 0;
	background-color:#fff;
	display: none;
	border-radius: 5px;
}
.hid-cont{
	border-bottom: 1px solid #bbbbbb;
	line-height: 30px;
	font-size: 16px;
	padding-bottom: 5px;
	padding-left:15px;
	padding-right:15px;
	color:#777;
	text-align:center;
	overflow: word-wrap;
}
.hid-cont img{
	width:18px;
	margin-right: 4px;
	position: relative;
	top:4px;
}
.hid-sub{
	cursor: pointer;
	line-height:25px;
	font-size: 16px;
	text-align: center;
	color:#777;
}


.mui-checkbox input[type=checkbox]:before, .mui-radio input[type=radio]:before{
	    font-size: 24px;
	    position: relative;
    	top: -4px;
    	left:-2px;
}
.mui-checkbox.mui-left label, .mui-radio.mui-left label{
	padding-left:37px;
}
.mui-input-row label{
	padding:6px 15px;
}