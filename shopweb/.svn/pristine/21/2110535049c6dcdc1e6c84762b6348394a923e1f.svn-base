.hid-pl{
	width: 100%;
	padding: 9px 0 0 0;
	background: #fff;
	display: none;
}
.hid-title{
	border-bottom: 1px solid #bbbbbb;
	line-height: 25px;
	font-size: 16px;
	padding-left: 15px;
	padding-bottom: 5px;
}
.hid-con{
	text-align: center;
	padding-top: 10px;
	padding-bottom: 10px;
}
.hid-con input{
	width: 90%;
	height: 30px;
	line-height: 30px;
	font-size: 14px;
	border-radius: 5px;
	margin-top: 5px;
	margin-bottom: 5px;
	color: #000;
}
.hid-sub{
	border-top:1px solid #bbbbbb;
	cursor: pointer;
	line-height:40px;
	font-size: 16px;
	text-align: center;
}
