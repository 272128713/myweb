* {
	margin: 0;
	padding: 0;
}
html,body{
	height:100%;
}
.clearfix{
	clear: both;
}
.container{
	width:100%;
	height:100%;
	background:#fff;
	position:relative;
}
.header{
	width:100%;
	height:40px;
	position:fix;
	top:0;
	padding-top:6px;
	padding-bottom:6px;
	background:#e8e8e8;
}
.header .header_con{
	width:100%;
}
.header_con .sort,.header_con .order{
	width:14.7%;
	float:left;
	text-align:center;
}
.header_con input{
	width:70.6%;
	height:28px;
	float:left;
	border:1px solid #b7b8bb;
	border-radius:5px;
	padding-top:5px;
	padding-bottom:5px;
	background:rgba(255,255,255,0) url(../images/search.png);
	background-repeat: no-repeat;
	background-position:2% center;
	background-size: 14px 14px;
	font-size:14px;
	color: #b0b0b0;
	font-family: "微软雅黑";
	text-align: left;
	margin:0;
	position:relative;
    text-indent: 1em;
}

