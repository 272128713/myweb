.container
{
	height:100%;
	background:#e8e8e8;
	position:relative;
	
}
.intro
{
	background:#fff;
	width:100%;
	line-height:15px;
	padding:12px 2.5% 12px 0;
}
.intro-rectangle
{
	width:5px;
	height:15px;
	background-color:#ff0000;
	float:left;
}
.intro-text
{
	font-size:15px;
	float:left;
	margin-left:7px;
	color:#000;
}
.left-part
{
	float:left;
}
.xz
{
	float:right;
}
.doctor-list
{
	padding-bottom:10px;
	cursor: pointer;
	min-height:600px;
}
.textarea
{
	margin:0 auto;
	background:#fff;
	margin-top:44px;
}
.text
{
	width:100%;
	padding:9px 2.5% 9px 2.5%;
	border-top:5px solid #e8e8e8;
}
.title1
{
	font-size:16px;
	color: #000;
	line-height:16px;
}
.title2
{
	font-size:14px;
	margin-top:12px;
	color: #000;
	line-height:14px;
}
.title3
{
	font-size:12px;
	margin-top:11px;
	color:#8b8b8b;
	line-height:12px;
}
.content
{
	padding-top:20px;
	padding-bottom:8px;
	background:#fff;
	border-bottom: 1px solid #bbb;
}
.content-inner
{
	margin:0 auto;
	width:95%;
}
.content-inner img
{
	width:17%;
	float:left;
}
.doctor_info
{
	width:83%;
	float:left;
	padding-left:10px;
	
}
.clear
{
	clear:both;
}
.xingming
{
	font-size:16px;
	color: #000;
	margin-bottom:9px;
	line-height:16px;
}

.doctor_info .orange
{
	font-size:12px;
	color:#7bebdd;
	margin-left:8%;
}
.doctor_info .red
{
	font-size:12px;
	color:#777;
	float:right;
}
.keshi
{
	font-size:12px;
	color:#787878;
	margin-bottom:7px;
	line-height:12px;
}
.yishi
{
	font-size:12px;
	margin-left:7%;
	color:#787878;
}
.address
{
	font-size:12px;
	line-height:18px;
	padding-right:15%;	
	color:#787878;
}
.area
{
	font-size:12px;	
	color:#000;
	line-height:12px;
	margin-top:3px;
}
.border-line{
	width:100%;
	height:5px;
	background: #e8e8e8;
}
.img-hb
{
	padding-top:10px;
	padding-bottom:10px;
	background:#e8e8e8;
}
.img-hb img{
	width:190px;
	position:relative;
	top:1.5px;
}
.service
{
	background:#fff;
	padding-top:12px;
	padding-bottom:12px;
	padding-left:2.5%;
	padding-right:2.5%;
	line-height:15px;
}
.service .sev{
	height:15px;
	line-height:15px;
}
.service .serv .sd
{
	font-size:15px;
	float:left;
	color: #000;
}
.service .know
{
	float:right;
	position:relative;
	top:2px;
}
.service .know span:hover{
	color: #000;
}
.know img{
	width: 13px;
	margin-right:-3px;
}
.know span{
	position:relative;
	font-size:13px;
	top:-2px;
}
.service-other
{
	background:#fff;
	padding-top:12px;
	padding-bottom:12px;
	padding-left:2.5%;
	line-height:15px;
	position: relative;
}
.service-other .red-line{
	width: 4px;
	height: 15px;
	background: red;
	position: absolute;
	left: 0;
	
}
.service-other p 
{
	font-size:15px;
	color:#000;
}
.jieshao
{
	background:#e8e8e8;
	padding-top:10px;
	padding-bottom:10px;
}
.wenzi
{
	font-size:14px;
	width:95%;
	margin:0 auto;
	text-indent: 2em;
	min-height:280px;
}
.btn
{
	padding-top:10px;
	padding-bottom:10px;
	width:100%;
	background: #fff;
	position: fixed;
	bottom: 0;
	box-shadow:  0 -2px 4px #e1e1e1;
}
.btn1
{
	width: 95%;
	margin: auto;
}
.canyu
{
	font-size: 16px;
	color:#fff;
	background:#30c6df;
	padding:8px 8px;
	border-radius:5px;
	float:left;
	cursor: pointer;
}
.canyu:hover{
	position: relative;
	left: 1px;
	top: 1px;
}
.btn a:hover{
	color: #fff;
}
.detail-list{
	padding-bottom: 52px;
}
.detail-list img{
	width: 100%;
}
.buy
{
	font-size: 16px;
	color:#fff;
	background:#30c6df;
	padding:6px 6px;
	border-radius:5px;
	float:right;
	cursor: pointer;
}
.buy:hover{
	position: relative;
	left: 1px;
	top: 1px;
}
.arrow-right{
	width: 44px;
    height: 44px;
    position: absolute;
    right: 0;
    top: 30px;
    padding-left: 22px;
    cursor: pointer;
}
.arrow-right img{
	width:22px;
}