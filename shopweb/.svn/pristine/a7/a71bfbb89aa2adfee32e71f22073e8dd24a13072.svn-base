.news-list{
	padding: 10px 0 10px 0;
	border-bottom: 1px solid #bbbbbb;
	position: relative;
}
.news-list .p1{
	color: #000;
	font-size: 16px;
	line-height: 35px;
	position:relative;
}
.news-list .p1 b{
	color:#fb7171;
	margin-right: 4px;
	font-size:16px;
}
.news-list .p1 img{
	width: 16px;
	position: relative;
	top:2px;
	margin-right: 4px;
}
.news-list .p1 span{
	position: absolute;
	right: 0;
}
.p1 .news{
	width:8px;
	height:8px;
	border-radius:50px;
	background:#e45335;
	position:absolute;
	top:4px;
	left:10px;
}
