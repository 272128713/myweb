.title{
	width:500px;
	float:left;
	margin-top: 55px;
}
.search{
	padding-left: 20px;
	float:right;
	margin-top: 55px;
}
.title .line{
	width:3px;
	height:34px;
	background:#4dbbaa;
	float:left;
	position:relative;
	top:4px;
}
.title span{
	display: block;
	float:left;
	color:#595959;
	font-size: 42px;
	line-height:42px;
	font-family: myfont4;
	margin-left: 33px;
}
.search input{
	width:328px;
	height:43px;
	color:#959494;
	font-size:22px;
	border:0;
	border:1px solid #d5d5d5;
	background:#e9e9e9 url(../images/search.png) no-repeat;
	background-position:15px center;
	padding: 0 30px 0 60px;
	float:left;
	outline: none;
}
.search .search_btn{
	width:60px;
	height:45px;
	color:#fff;
	font-size:22px;
	line-height:45px;
	background:#00d5c3;
	float:left;
	text-align: center;
	cursor: pointer;
}
.skydynamic{
	width:1000px;
	height:57px;
	background:#eaeaea;
	padding-top:34px;
}
.first{
	margin-top:32px;
}
.after{
	height:345px;
	padding-top:0;
}
.skydynamic .top{
	width:1000px;
	height:56px;
}
.skydynamic .top .header{
	position:relative;
}
.skydynamic .top .header img{
	position:absolute;
	top:0;
	left:-20px;
	z-index:1;
}
.skydynamic .top .header .text{
	color:#fff;
	font-size:22px;
	font-family: myfont2;
	position:absolute;
	top:8px;
	left:28px;
	z-index:2;
}
.skydynamic .top .subtitle{
	position:relative;
	left:175px;
	top:25px;
	color:#a3a3a3;
	font-size:16px;
	font-family: myfont3;
}
.skydynamic .top .right{
	float:right;
	position:relative;
	top:25px;
	cursor: pointer;
}
.skydynamic .top .right a{
	text-decoration: none;
}
.skydynamic .top .right a:hover{
	text-decoration: underline;
}
.skydynamic .top .more{
	color:#4dbbaa;
	font-size: 16px;
	font-family: myfont2;
	display:block;
	float:left;
}
.skydynamic .top .right img{
	float:right;
	position:relative;
	top:3px;
	margin-left:12px;
}
.skydynamic .dis_line{
	margin-left:30px;
	position:relative;
	top:-2px;
	display:block;
}
.inner{
	width:100%;
	margin:0 auto;
	padding-top:50px;
    padding-bottom: 40px;
    border-bottom:2px dotted #c0c0c0;
    background:#eaeaea;
    margin-bottom: 20px;
}
.after .inner{
	padding-top:65px;
}
.inner .date{
	width:94px;
	height:94px;
	float:left;
	border:3px solid #4dbbaa;
	border-radius: 50%;
	color:#4dbbaa;
	background:#fff;
	text-align: center;
	margin-left: 25px;
	margin-right: 25px;
}
.inner .outline{
	width:520px;
	float:left;
}
.inner .date .p1{
	font-size: 32.75px;
	border-bottom:3px solid #4dbbaa;
	top:8px;
	position:relative;
}
.inner .date .p2{
	font-size:15.59px;
	margin-top:8px;
	top:8px;
	position:relative;
}
.inner .outline .biaoti{
	font-size:18.02px;
	font-family: "方正兰亭中粗黑_GBK";
	color:#4c4c4c;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.inner .tupian img{
	width:280px;
	height:110px;
	float:left;
    margin-top: 20px;
    margin-right:50px
}
.inner .outline .news_content{
	font-family: "兰亭黑_GBK";
    float:left;
    width: 411px;
	font-size:14.41px;
	line-height:28px;
	color:#666666;
	margin-top:18px;
	display:-webkit-box;
	-webkit-box-orient:vertical;
	-webkit-line-clamp: 4;
	overflow:hidden;
}
.after .inner .outline .news_content{
	display:-webkit-box;
	-webkit-box-orient:vertical;
	-webkit-line-clamp: 6;
	overflow:hidden;
}
.inner .outline .seedetail{
	width:85px;
	height:25px;
	line-height: 25px;
	border:2px solid #4dbbaa;
	font-size: 12.61px;
	font-family: myfont2;
	color:#4dbbaa;
	text-align: center;
	margin-top:35px;
    cursor: pointer;
}
.inner .outline .seedetail:hover{
    background: #4dbbaa;
    color: #fff;
}
.inner .outline .news2{
	margin-top:15px;
}
.inner .outline .detail2{
	margin-top:15px;
}
.inner2{
	padding-bottom:35px;
	border-bottom:2px dotted #bfbfbf;
}
.inner3{
	padding-top:38px;
	padding-bottom:45px;
}

.listbox{
    width: 811px;
}