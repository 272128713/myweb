* {
	margin: 0;
	padding: 0;
}
.container{
	width:100%;
	background-image: url(../images/bg.png);
	background-size: 100% 100%;
	z-index:1;
	position:absolute;
	top:0;
	left:0;
}
.sec-bot{
	width:86.94%;
	background:#fff;
	border-radius:5px;
	position:absolute;
	top:77.62%;
	left:6.667%;
	z-index: 0;
	padding-top:10px;
	padding-bottom:10px;
}
.swiper-container{
	margin-left: 6.39%;
	margin-right: 6.39%;
}
.swiper-wraper{
	
}
.swiper-slide{
	width:100%;
}
.swiper-slide .pull-left{
	width:21.442%;
	float:left;
	border:1px solid #cdcdcd;
	margin-right:1%;
	padding-top:5px;
	padding-left:1%;
	padding-right: 1%;
}
.swiper-slide .pull-left:last-child{
	margin-right:0;
}
.pull-left img{
	width:100%;
}
.pull-left p{
	color:#000;
	font-size:12px;
	text-align:center;
	line-height:12px;
	
}
.header{
	width:91.67%;
	margin:0 auto;
	margin-top:16px;
	display:block;
}
.detail{
	width:86.94%;
	background:#fff;
	padding-top:20px;
	padding-bottom:10px;
	border-radius:5px;
	top:36.21%;
	left:6.667%;
	position:absolute;
}
.detail img{
	width:73.323%;
	left:6.94%;
	position:relative;
}
.baoming{
	width:86.94%;
	background:#fff;
	border-radius:5px;
	padding-top:10px;
	padding-bottom:10px;
	top:60%;
	left:6.667%;
	position:absolute;
}
.baoming .button{
	width:21.25%;
	right:10.7%;
	position:absolute;
}
.baoming .bmtext{
	width:13.26%;
	left:11.182%;
	position:relative;
	display:block;
}
.baoming p{
	top:2px;
	left:11.182%;
	position:relative;
}
.count{
	font-size:24px;
	color:#f5582d;
}