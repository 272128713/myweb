	<script>
		apiready = function() {
			$header = $api.dom('.header');
			$api.fixStatusBar($api.dom('.header'));
			$header_h = $api.offset($header).h;
			name = api.pageParam.name;
			fname = api.pageParam.fname;
			console.log(name);
			console.log(fname);
			var jsfun = 'execlose();';
			api.execScript({
	            name : name,
	            frameName : fname,
	            script: jsfun
            });
			api.openFrame({
				name : 'flogin',
				url : 'frame_login.html',
				rect : {
					x : 0,
					y : $header_h,
					w : 'auto',
					h : 'auto'
				}
			});
		}
	</script>
