}
.mui-slider-indicator .mui-active.mui-indicator {
    background: #dd403b;
    box-shadow:0 0 1px 1px rgba(221,64,59,.7);
}
.st-menu{
	position: fixed;
	top:40px;
	left: 0;
	width: 30%;
	color: #000;
	z-index: 9999;
	
	padding: 4px;
	text-align: center;
	-webkit-transition: all 0.5s;
	transition: all 0.5s;
}
.st-menu .bg{
	position: absolute;
	top:0;
	right: 0;
	width: 100%;
	z-index: 9000;
    height: 100%;
    background-color:#676767;
    filter:Alpha(opacity=50);
    background-color: rgba(0,0,0,0.5);
}
.st-menu p{
	position: relative;
	z-index: 9001;
	border-bottom: 1px solid #fff;
	margin: 0;
	
}
.st-menu a{
	line-height: 3.5em;
	font-size: 12px;
	color: #fff;
	display: block;
	-webkit-transition: background 0.3s, box-shadow 0.3s;
	transition: background 0.3s;
}
.st-menu img{
	width: 1.5em;
	position: relative;
	top:0.4em;
	margin-right: 0.3em;
}
.st-effect-1.st-menu {
	visibility: visible;
	-webkit-transform: translate3d(-100%, 0, 0);
	transform: translate3d(-100%, 0, 0);
}

.st-effect-1.st-menu-open .st-effect-1.st-menu {
	visibility: visible;
	-webkit-transform: translate3d(0, 0, 0);
	transform: translate3d(0, 0, 0);
}

.st-effect-1.st-menu::after {
	display: none;
}
.st-menu-open .st-pusher::after{
	width: 100%;
	height: 100%;
	opacity: 1;
	transition: opacity 0.5s;
}
.st-pusher::after {
	position: absolute;
	top: 0;
	right: 0;
	width: 0;
	height: 0;
	background: rgba(0,0,0,0.4);
	content: '';
}