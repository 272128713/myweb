.retitle{
	width:50%;
	margin:0 auto;
	height:30px;
	border:1px solid #bbb;
	border-radius:50px;
}
.retitle span{
	display:block;
	width:100%;
	height:30px;
	line-height:27px;
	text-align:center;
	float:left;
}
.consumList{
	width:95%;
	margin:auto;
	padding-top:18px;
	padding-bottom:17px;
	border-bottom:1px solid #dcdcdc;
}
.consumList img{
	width:30%;
	float:left;
}
.consumList .listintro{
	width:65%;
	margin-left:3%;
	float:left;
	line-height:25px;
}
.list-clear{
	clear:both;
}
.listintro .p3{
	font-size:14px;
	color:#ff0000;
	line-height:14px;
	margin-top:8px;
}
.listintro .name{
	font-size:14px;
	line-height:21px;
	color:#000;
}
.delivery .p5{
	font-size:12px;
	line-height:12px;
	margin-top:13px;
	color:#606060;
}
