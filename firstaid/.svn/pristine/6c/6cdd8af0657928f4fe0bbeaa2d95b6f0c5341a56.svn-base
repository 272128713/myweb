    i = 0;
    $('.pro_list').click(function(){
        sl = $(this).attr("sl");
        if(sl != 1){
            css = $(this).children('.proinfo').children('span');
            css.css("background-image","url('./images/xuan1.png')");
            $(this).attr("sl","1");
            i ++;
            $('#count').html("已选"+i+"件");
        }else if(sl==1){
            css = $(this).children('.proinfo').children('span');

            css.css("background-image","url('./images/xuan.png')");
            $(this).attr("sl","0");
            i--;
            $('#count').html("已选"+i+"件");
        }
    });
